{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        },
        {
          "url": "http://*"
        }
      ]
    },
    "core:resources:default",
    "core:event:default",
    "dialog:allow-confirm",
    "log:default",
    "fs:scope-home-recursive",
    "os:allow-os-type",
    "core:window:default",
    "fs:default",
    "shellx:allow-open",
    "os:default",
    "os:allow-locale",
    "upload:default",
    "os:allow-platform",
    "dialog:allow-message",
    "os:allow-hostname",
    "fs:write-all",
    "network:allow-all",
    "dialog:default",
    "shellx:allow-fix-path-env",
    "core:app:default",
    "dialog:allow-open",
    "clipboard:read-all",
    "os:allow-arch",
    "fs:read-all",
    "core:tray:default",
    "os:allow-exe-extension",
    "dialog:allow-save",
    "shellx:allow-spawn",
    "notification:default",
    "os:allow-family",
    "core:path:default",
    "core:menu:default",
    "shellx:allow-execute",
    "shellx:allow-kill",
    "shellx:allow-kill-pid",
    "os:allow-version",
    "clipboard:write-all",
    "dialog:allow-ask",
    "system-info:allow-all",
    "core:image:default"
  ]
}
